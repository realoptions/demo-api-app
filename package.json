{
  "name": "demo-api-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://realoptions.github.io/demo-api-app",
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "gh-pages": "^2.0.1",
    "option-charts": "0.0.5",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.0",
    "react-async": "^3.8.1",
    "react-dom": "^16.7.0-alpha.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "realoptions-node-sdk": "0.0.5",
    "redux": "^4.0.1",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "download-docs": "./mockserver/fetch_yaml.sh",
    "mock-api-background": "nohup ./mockserver/apisprout ./mockserver/openapi.yaml &",
    "mock-api": "./mockserver/apisprout ./mockserver/openapi.yaml"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write --no-semi",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "1.15.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.js",
      "!src/serviceWorker.js",
      "!src/index.js"
    ]
  }
}
